<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- Hive Metastore URI -->
    <property>
        <name>hive.metastore.uris</name>
        <value>thrift://metastore:9083</value>
    </property>

    <!-- Storage Handler -->
    <property>
        <name>hive.default.fileformat</name>
        <value>Parquet</value>
    </property>
</configuration>
