<?xml version="1.0"?>
<configuration>
    <!-- ZooKeeper quorum -->
    <property>
        <name>hbase.zookeeper.quorum</name>
        <value>zookeeper</value>
    </property>

    <!-- ZooKeeper client port -->
    <property>
        <name>hbase.zookeeper.property.clientPort</name>
        <value>2181</value>
    </property>
</configuration>
